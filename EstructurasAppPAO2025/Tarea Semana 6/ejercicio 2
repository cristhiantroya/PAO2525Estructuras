public class Nodo
{
    public int Data;
    public Nodo? Next;

    public Nodo(int data)
    {
        Data = data;
        Next = null;
    }
}
public class ListaSimple
{
    private Nodo? head;

    public void InsertarFinal(int dato)
    {
        Nodo nuevoNodo = new Nodo(dato);
        if (head == null)
            head = nuevoNodo;
        else
        {
            Nodo actual = head;
            while (actual.Next != null)
                actual = actual.Next;
            actual.Next = nuevoNodo;
        }
    }
 public void DibujarLista()
    {
        Nodo? actual = head;
        Console.Write("head -->");
        while (actual != null)
        {
            Console.Write($" [ {actual.Data} | * ]");
            if (actual.Next != null)
                Console.Write(" -->");
            actual = actual.Next;
        }
        Console.WriteLine(" --> null");
    }

    public void EliminarFueraDeRango(int min, int max)
    {
        Nodo? actual = head;
        Nodo? anterior = null;

        while (actual != null)
        {
            if (actual.Data < min || actual.Data > max)
            {
                if (anterior == null)
                {
                    // El nodo a eliminar es el primero
                    head = actual.Next;
                    actual = head;
                }
                else
                {
                    // Nodo en medio o final
                    anterior.Next = actual.Next;
                    actual = anterior.Next;
                }
            }
            else
            {
                anterior = actual;
                actual = actual.Next;
            }
        }
    }
}
